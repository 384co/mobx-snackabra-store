{
  "name": "mobx-snackabra-store",
  "version": "1.0.24",
  "description": "A Mobx implementation of a state store for use in React PWA's",
  "main": "./index.js",
  "module": "./index.js",
  "scripts": {
    "test": "jest --runInBand --coverage --verbose --detectOpenHandles --forceExit",
    "build": "./node_modules/.bin/babel src --out-dir lib",
    "docs": "./node_modules/.bin/jsdoc -d docs -c jsdoc.json",
    "start": "npm run build && npm run watch",
    "nodemon": "nodemon --watch ./src -L -e js --ignore node_modules/ --exec \"npm run build\"",
    "watch": "concurrently -k -p \"[{name}]\" -n \"Watcher\" -c \"green.bold,cyan.bold,magenta.bold,white.bold\"  \"npm run nodemon\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/384co/mobx-snackabra-store.git"
  },
  "keywords": [
    "snackabra"
  ],
  "author": "384 Inc",
  "license": "GPL-3.0-only",
  "bugs": {
    "url": "https://github.com/384co/mobx-snackabra-store/issues"
  },
  "homepage": "https://github.com/384co/mobx-snackabra-store#readme",
  "peerDependencies": {
    "@types/react": "^17.0.0 || ^18.0.0",
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0"
  },
  "dependencies": {
    "mobx": "^6.7.0",
    "react": "^17.0.0 || ^18.0.0",
    "snackabra": "https://github.com/384co/snackabra-jslib.git#development"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@jest/globals": "^29.4.2",
    "@types/jest": "^29.4.0",
    "@types/jest-environment-puppeteer": "^5.0.3",
    "@types/node": "^18.13.0",
    "@types/puppeteer": "^7.0.4",
    "babel-jest": "^29.4.2",
    "concurrently": "^6.0.0",
    "fake-indexeddb": "^4.0.1",
    "jest": "^29.4.2",
    "jest-environment-jsdom": "^29.4.2",
    "jest-puppeteer": "^7.0.0",
    "jsdoc": "^4.0.0",
    "nodemon": "^2.0.14",
    "puppeteer": "^19.6.3",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.5"
  }
}
